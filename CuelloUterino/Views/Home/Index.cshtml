@section pagespecific {

    <script type="text/javascript">

        $(document).ready(function () {


            $("#insertarMuestra").click(function () {
                $('#content').waitMe({
                    effect: 'ios',
                    text: '',
                    bg: 'rgba(255, 255, 255, 0.1)',
